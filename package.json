{
    "name": "@repo/prisma-kysely",
    "version": "1.8.0",
    "description": "Generate Kysely database types from a Prisma schema",
    "repository": {
        "url": "git+https://github.com/valtyr/prisma-kysely.git"
    },
    "license": "MIT",
    "author": {
        "name": "Valtyr Orn Kjartansson",
        "url": "http://valtyr.is"
    },
    "contributors": [
        {
            "name": "Alistair Smith",
            "url": "https://alistair.sh"
        },
        {
            "name": "Arthur Fiorette",
            "url": "https://arthur.place"
        }
    ],
    "main": "dist/generator.js",
    "bin": {
        "prisma-kysely": "dist/bin.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc && node ./scripts/fix-imports.js",
        "dev": "concurrently \"tsc --watch\" \"node ./scripts/watch-imports.js\"",
        "fix": "prettier --write .",
        "lint": "eslint ./src",
        "prepack": "pnpm build",
        "release": "pnpm build && pnpm changeset publish",
        "start": "node dist/bin.js",
        "test": "pnpm build && vitest --passWithNoTests --coverage",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@mrleebo/prisma-ast": "^0.12.1",
        "@prisma/generator-helper": "6.10.1",
        "@prisma/internals": "6.10.1",
        "code-block-writer": "^12.0.0",
        "typescript": "^5.8.3",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@changesets/cli": "^2.29.5",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/node": "24.0.3",
        "@types/pg": "^8.15.4",
        "@types/prettier": "3.0.0",
        "@typescript-eslint/eslint-plugin": "^8.34.1",
        "@typescript-eslint/parser": "^8.34.1",
        "@typescript-eslint/typescript-estree": "^8.34.1",
        "@vitest/coverage-v8": "^3.2.4",
        "eslint": "^9.29.0",
        "kysely": "^0.28.3",
        "mysql2": "^3.14.1",
        "pg": "^8.16.2",
        "prettier": "^3.5.3",
        "prisma": "6.10.1",
        "ts-patch": "^3.3.0",
        "typescript-transform-paths": "^3.5.5",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    },
    "peerDependencies": {
        "prisma": "~6.10"
    }
}
