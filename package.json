{
  "name": "prisma-kysely",
  "version": "1.8.0",
  "description": "Generate Kysely database types from a Prisma schema",
  "repository": {
    "url": "git+https://github.com/valtyr/prisma-kysely.git"
  },
  "license": "MIT",
  "main": "dist/generator.js",
  "bin": {
    "prisma-kysely": "dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tspc",
    "dev": "tspc --watch",
    "fix": "prettier --write .",
    "lint": "eslint ./src",
    "prepack": "yarn build",
    "release": "yarn build && yarn changeset publish",
    "start": "node dist/bin.js",
    "test": "yarn build && vitest --passWithNoTests --coverage",
    "typecheck": "tspc --noemit"
  },
  "dependencies": {
    "@mrleebo/prisma-ast": "^0.12.1",
    "@prisma/generator-helper": "6.8.2",
    "@prisma/internals": "6.8.2",
    "typescript": "^5.8.3",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "22.15.29",
    "@types/pg": "^8.10.2",
    "@types/prettier": "3.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@typescript-eslint/typescript-estree": "^8.32.1",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^9.28.0",
    "kysely": "^0.28.2",
    "mysql2": "^3.14.1",
    "pg": "^8.11.3",
    "prettier": "^3.5.3",
    "prisma": "6.8.2",
    "ts-patch": "^3.0.2",
    "typescript-transform-paths": "^3.5.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "peerDependencies": {
    "prisma": "^6.8.2"
  },
  "peerDependenciesMeta": {
    "prisma": {
      "optional": true
    }
  },
  "packageManager": "yarn@1.22.22"
}
