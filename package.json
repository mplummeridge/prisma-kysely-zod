{
  "name": "prisma-kysely",
  "version": "1.8.0",
  "description": "Generate Kysely database types from a Prisma schema",
  "repository": {
    "url": "https://github.com/valtyr/prisma-kysely"
  },
  "license": "MIT",
  "main": "dist/generator.js",
  "bin": {
    "prisma-kysely": "dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tspc",
    "dev": "tspc --watch",
    "edit": "code .",
    "fix": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "eslint ./src",
    "prepack": "yarn build",
    "release": "yarn build && yarn changeset publish",
    "start": "node dist/bin.js",
    "test": "yarn build && vitest --passWithNoTests --coverage",
    "typecheck": "tspc --noemit"
  },
  "prettier": {
    "importOrder": [
      "^\\./env$",
      "<THIRD_PARTY_MODULES>",
      "^~/.*$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  },
  "dependencies": {
    "@mrleebo/prisma-ast": "^0.7.0",
    "@prisma/generator-helper": "5.3.1",
    "@prisma/internals": "5.3.1",
    "typescript": "^5.8.3",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "22.15.1",
    "@types/pg": "^8.10.2",
    "@types/prettier": "2.7.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@typescript-eslint/typescript-estree": "^8.32.1",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^8.49.0",
    "kysely": "^0.26.3",
    "mysql2": "^3.6.1",
    "pg": "^8.11.3",
    "prettier": "^2.8.8",
    "prisma": "5.3.1",
    "ts-patch": "^3.0.2",
    "typescript-transform-paths": "^3.5.5",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^3.1.3"
  },
  "packageManager": "yarn@1.22.22"
}
